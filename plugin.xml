<?xml version="1.0" encoding="utf-8" ?>
<plugin id="cordova-plugin-cookies" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>CookiesPlugin</name>
    <js-module name="CookiesPlugin" src="www/CookiesPlugin.js">
        <clobbers target="cordova.plugins.CookiesPlugin" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="CookiesPlugin">
                <param name="ios-package" value="CookiesPlugin" />
            </feature>
        </config-file>
        <source-file src="src/ios/CookiesPlugin.m" />
    </platform>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="CookiesPlugin">
                <param name="android-package" value="cordova.plugins.cookies.CookiesPlugin" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/CookiesPlugin.java" target-dir="src/cordova/plugins/cookies/" />
    </platform>
</plugin>